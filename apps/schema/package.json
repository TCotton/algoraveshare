{
  "name": "@repo/schema",
  "version": "1.0.0",
  "description": "schema used in both frontend and backend",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --config ./eslint.config.ts --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --config ./eslint.config.ts --ext .js,.ts,.jsx,.tsx --fix",
    "preinstall": "npx only-allow pnpm"
  },
  "exports": {
    "./projects": {
      "types": "./src/projects.schema.ts",
      "default": "./dist/src/projects.schema.js"
    },
    "./tagAssignments": {
      "types": "./src/tagAssignments.schema.ts",
      "default": "./dist/src/tagAssignments.schema.js"
    },
    "./tags": {
      "types": "./src/tags.schema.ts",
      "default": "./dist/src/tags.schema.js"
    },
    "./users": {
      "types": "./src/users.schema.ts",
      "default": "./dist/src/users.schema.js"
    },
    "./usersSchema": {
      "types": "./src/usersSchema.schema.ts",
      "default": "./dist/src/usersSchema.schema.js"
    }
  },
  "dependencies": {
    "effect": "^3.18.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/validator": "^13.15.4",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "typescript-eslint": "^8.46.2",
    "validator": "^13.15.20"
  }
}
