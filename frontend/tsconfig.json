{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "reset.d.ts",
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "eslint.config.ts"
  ],
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "verbatimModuleSyntax": true
  }
}